generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model USERS {
    id          Int       @id @default(autoincrement())
    name        String
    cpf         String
    access_type String
    team        String @default("Colaborador")
    MESSAGE     MESSAGES[]
    createAt    DateTime @default(now())
}

model MESSAGES {
   id       Int       @id @default(autoincrement())
   message  String
   team     String
   user     USERS  @relation(fields: [userId], references: [id])
   userId   Int
   createAt DateTime @default(now())
}
